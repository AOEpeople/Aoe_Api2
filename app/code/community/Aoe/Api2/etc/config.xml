<?xml version="1.0"?>
<config>
    <modules>
        <Aoe_Api2>
            <version>0.0.1</version>
        </Aoe_Api2>
    </modules>
    <global>
        <helpers>
            <Aoe_Api2>
                <class>Aoe_Api2_Helper</class>
            </Aoe_Api2>
        </helpers>
        <models>
            <Aoe_Api2>
                <class>Aoe_Api2_Model</class>
            </Aoe_Api2>
            <api2>
                <rewrite>
                    <server>Aoe_Api2_Model_Server</server>
                    <acl>Aoe_Api2_Model_Acl</acl>
                </rewrite>
            </api2>
        </models>
        <api2>
            <auth_adapters>
                <customer_session module="Aoe_Api2" translate="label">
                    <model>Aoe_Api2/Auth_Adapter_Session</model>
                    <label>Session</label>
                    <enabled>1</enabled>
                    <order>1000</order>
                </customer_session>
            </auth_adapters>
        </api2>
    </global>
</config>
